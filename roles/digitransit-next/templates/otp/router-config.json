{
  "routingDefaults": {
    "walkSpeed": 1.3,
    "transferSlack": 100,
    "maxTransfers": 4,
    "waitReluctance": 0.4,
    "waitAtBeginningFactor": 0.7,
    "walkReluctance": 1.75,
    "stairsReluctance": 1.65,
    "walkBoardCost": 540,
    "comment": "Ignore walkOnStreetReluctance as implementation is too simple and causes unexpected detours",
    "ignored_walkOnStreetReluctance": 1.5,
    "carParkCarLegWeight": 2,
    "itineraryFiltering": 1.0,
    "allowKeepingRentedBicycleAtDestination": true,
    "keepingRentedBicycleAtDestinationCost": 180,
    "itineraryFilters" : {
      "parkAndRideDurationRatio": 0.6
    }
  },
  "vectorTileLayers": [
    {
      "name": "stops",
      "type": "Stop",
      "mapper": "Digitransit",
      "maxZoom": 20,
      "minZoom": 14,
      "cacheMaxSeconds": 600
    },
    {
      "name": "stations",
      "type": "Station",
      "mapper": "Digitransit",
      "maxZoom": 20,
      "minZoom": 12,
      "cacheMaxSeconds": 600
    },
    {
      "name": "citybikes",
      "type": "BikeRental",
      "mapper": "Digitransit",
      "maxZoom": 20,
      "minZoom": 14,
      "cacheMaxSeconds": 60
    }
  ],
  "updaters": [
    {% if enable_cargobike %}
    {
      "id": "cargo-bike",
      "type": "bike-rental",
      "frequencySec": 600,
      "sourceType": "gbfs",
      "url": "https://raw.githubusercontent.com/stadtnavi/static-gbfs-feeds/master/cargo-bike/gbfs.json",
      "network": "cargo-bike",
      "allowKeepingRentedBicycleAtDestination": true
    },
    {% endif %}
    {
      "id": "regiorad-bike-rental",
      "type": "bike-rental",
      "frequencySec": 60,
      "sourceType": "gbfs",
      "url": "https://gtfs.mfdz.de/gbfs/regiorad_stuttgart/",
      "network": "regiorad",
      "rentalType": "station-based-with-temporary-drop-off",
      "allowKeepingRentedBicycleAtDestination": true
    },
    {
      "id": "taxi-stands",
      "type": "bike-rental",
      "frequencySec": 600,
      "sourceType": "gbfs",
      "url": "https://api.stadtnavi.de/herrenberg/taxi/",
      "network": "taxi",
      "routeAsCar": true
    },
    {
      "id": "car-sharing",
      "type": "bike-rental",
      "frequencySec": 600,
      "sourceType": "gbfs",
      "url": "https://raw.githubusercontent.com/stadtnavi/static-gbfs-feeds/master/car-sharing/",
      "network": "car-sharing",
      "routeAsCar": true
    }
  ]
}
