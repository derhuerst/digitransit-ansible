{
  "routingDefaults": {
    "walkSpeed": 1.3,
    "transferSlack": 100,
    "maxTransfers": 4,
    "waitReluctance": 0.4,
    "waitAtBeginningFactor": 0.7,
    "walkReluctance": 1.75,
    "stairsReluctance": 1.65,
    "walkBoardCost": 540,
    "comment": "Ignore walkOnStreetReluctance as implementation is too simple and causes unexpected detours",
    "ignored_walkOnStreetReluctance": 1.5,
    "carParkCarLegWeight": 2,
    "itineraryFiltering": 1.0,
    "allowKeepingRentedBicycleAtDestination": true,
    "keepingRentedBicycleAtDestinationCost": 180,
    "numItineraries": 10,
    "bikeTriangleSafetyFactor": 0.4,
    "bikeTriangleSlopeFactor": 0.3,
    "bikeTriangleTimeFactor": 0.3,
    "bannedBikeRentalNetworks": ["car-sharing", "taxi", "cargo-bike"],
    "itineraryFilters" : {
      "bikeRentalDistanceRatio": 0.5,
      "parkAndRideDurationRatio": 0.6
    }
  },
  "vectorTileLayers": [
    {
      "name": "stops",
      "type": "Stop",
      "mapper": "Digitransit",
      "maxZoom": 20,
      "minZoom": 14,
      "cacheMaxSeconds": 600,
      "expansionFactor": 0.5
    },
    {
      "name": "stations",
      "type": "Station",
      "mapper": "Digitransit",
      "maxZoom": 20,
      "minZoom": 12,
      "cacheMaxSeconds": 600,
      "expansionFactor": 0.5
    },
    {
      "name": "citybikes",
      "type": "BikeRental",
      "mapper": "Digitransit",
      "maxZoom": 20,
      "minZoom": 14,
      "cacheMaxSeconds": 60,
      "expansionFactor": 0.5
    }
  ],
  "updaters": [
    {% if enable_gtfs_rt %}
    {
      "type": "stop-time-updater",
      "frequencySec": 60,
      "sourceType": "gtfs-http",
      "url": "http://gtfsr.vvs.de:7788",
      "feedId": "hbg"
    },
    {
      "type": "real-time-alerts",
      "frequencySec": 60,
      "url": "http://gtfsr.vvs.de:7788",
      "feedId": "hbg"
    },
    {% endif %}
    {% if enable_cargobike %}
    {
      "id": "cargo-bike",
      "type": "bike-rental",
      "frequencySec": 600,
      "sourceType": "gbfs",
      "url": "https://backend.openbikebox.next-site.de/api/gbfs/2.2/2/gbfs.json",
      "network": "cargo-bike"
    },
    {
      "id": "alf",
      "type": "bike-rental",
      "frequencySec": 900,
      "sourceType": "gbfs",
      "url": "https://raw.githubusercontent.com/stadtnavi/static-gbfs-feeds/master/alf/gbfs.json",
      "network": "cargo-bike"
    },
    {
      "id": "bananologen",
      "type": "bike-rental",
      "frequencySec": 900,
      "sourceType": "gbfs",
      "url": "https://raw.githubusercontent.com/stadtnavi/static-gbfs-feeds/master/bananologen/gbfs.json",
      "network": "cargo-bike"
    },
    {
      "id": "gueltstein-mobil",
      "type": "bike-rental",
      "frequencySec": 900,
      "sourceType": "gbfs",
      "url": "https://raw.githubusercontent.com/stadtnavi/static-gbfs-feeds/master/gueltstein-mobil/gbfs.json",
      "network": "cargo-bike"
    },
    {% endif %}
    {
      "id": "regiorad-bike-rental",
      "type": "bike-rental",
      "frequencySec": 60,
      "sourceType": "gbfs",
      "url": "https://gtfs.mfdz.de/gbfs/regiorad_stuttgart/",
      "network": "regiorad",
      "rentalType": "station-based-with-temporary-drop-off",
      "allowKeepingRentedBicycleAtDestination": true
    },
    {
      "id": "taxi-stands",
      "type": "bike-rental",
      "frequencySec": 600,
      "sourceType": "gbfs",
      "url": "https://api.stadtnavi.de/herrenberg/taxi/",
      "network": "taxi",
      "routeAsCar": true
    },
    {
      "id": "car-sharing",
      "type": "bike-rental",
      "frequencySec": 600,
      "sourceType": "gbfs",
      "url": "https://raw.githubusercontent.com/stadtnavi/static-gbfs-feeds/master/car-sharing/",
      "network": "car-sharing",
      "routeAsCar": true
    },
    {
      "type": "vehicle-parking",
      "sourceType": "park-api",
      "frequencySec": 60,
      "feedId": "PAPI",
      "url": "https://api.stadtnavi.de/herrenberg/parking/parkapi.json"
    },
    {
      "type": "vehicle-parking",
      "sourceType": "open-bike-box",
      "frequencySec": 60,
      "feedId": "OBB",
      "url": "https://backend.openbikebox.next-site.de/api/v1/locations?format=list"
    }
  ]
}
